{
  "name": "mammatus/queue",
  "description": "\u23f1\ufe0f Queue manager",
  "license": "MIT",
  "type": "composer-plugin",
  "require": {
    "php": "^8.4",
    "ext-json": "^8.4",
    "composer-plugin-api": "^2.0",
    "eventsauce/object-hydrator": "^1.6.1",
    "mammatus/app": "dev-master",
    "mammatus/groups": "^0.1.0",
    "mammatus/kubernetes-attributes": "^1.0.0",
    "mammatus/kubernetes-contracts": "^1.0.0",
    "mammatus/life-cycle-events": "^2.3.0",
    "mammatus/queue-attributes": "^0.1.0",
    "mammatus/queue-contracts": "^0.1.0",
    "open-telemetry/api": "^1.4.0",
    "psr/container": "^1.1.2 || ^2",
    "psr/event-dispatcher": "^1.0",
    "psr/log": "^3.0.2",
    "queue-interop/queue-interop": "^0.8.1",
    "react/async": "^4.3",
    "react/event-loop": "^1.5",
    "react/promise": "^3.2",
    "react/promise-timer": "^1.11",
    "realodix/change-case": "^4.2",
    "roave/better-reflection": "^6.26",
    "wyrihaximus/broadcast-contracts": "^1.3",
    "wyrihaximus/generative-composer-plugin-tooling": "^1.3.0",
    "wyrihaximus/monolog-factory": "^2.0.0",
    "wyrihaximus/psr-3-callable-throwable-logger": "^2.4.0",
    "wyrihaximus/psr-3-context-logger": "^3.0.0",
    "wyrihaximus/ticking-promise": "^3.1.0"
  },
  "require-dev": {
    "bbqueue/bunny": "dev-main",
    "bunny/bunny": "0.6.0-alpha.2@dev",
    "wyrihaximus/async-test-utilities": "^10.6.0",
    "wyrihaximus/makefiles": "^0.7.14"
  },
  "autoload": {
    "psr-4": {
      "Mammatus\\DevApp\\Queue\\": "etc/dev-app/",
      "Mammatus\\Queue\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mammatus\\Tests\\Queue\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "drupol/composer-packages": true,
      "ergebnis/composer-normalize": true,
      "icanhazstring/composer-unused": true,
      "infection/extension-installer": true,
      "mammatus/app": true,
      "mammatus/groups": true,
      "mindplay/composer-locator": true,
      "phpstan/extension-installer": true,
      "wyrihaximus/broadcast": true,
      "wyrihaximus/makefiles": true,
      "wyrihaximus/test-utilities": true
    },
    "platform": {
      "php": "8.4.13"
    },
    "sort-packages": true
  },
  "extra": {
    "class": "Mammatus\\Queue\\Composer\\CodeGenerator",
    "mammatus": {
      "has-bootable": true,
      "has-groups": true,
      "queue": {
        "has-workers": true
      }
    },
    "php-di-definitions": {
      "di": [
        "etc/di/*.php"
      ]
    },
    "wyrihaximus": {
      "broadcast": {
        "has-listeners": true
      }
    }
  },
  "scripts": {
    "post-install-cmd": [
      "make on-install-or-update || true"
    ],
    "post-update-cmd": [
      "make on-install-or-update || true"
    ],
    "pre-autoload-dump": [
      "Mammatus\\Queue\\Composer\\CodeGenerator::findActions"
    ],
    "generate-config": [
      "Mammatus\\Queue\\Composer\\CodeGenerator::findActions"
    ]
  }
}
